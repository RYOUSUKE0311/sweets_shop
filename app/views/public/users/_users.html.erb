<table class="table table-striped">
  <thead>
    <tr>
      <th>id</th>
      <th><%= t "activerecord.attributes.user.name" %></th>
    </tr>
  </thead>
  <tbody>
    <% users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <% if current_admin %>
          <td><%= link_to user.name, admin_user_path(user) %></td>
        <% else %>
          <td><%= link_to user.name, user_path(user) %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>
<%= paginate users %>